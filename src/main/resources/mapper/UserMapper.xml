<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fan.mybatisplus.mapper.UserMapper">
    <select id="selectMapSelfDefined" resultType="map">
        select id, name, age, email from user where id = #{id}
    </select>

<!--    Page<User> selectPageUseSql(@Param("page") Page<User> page, @Param("age") Integer age);-->
    <select id="selectPageUseSql" resultType="User">
        select uid, user_name, age from t_user where age > #{age}
    </select>
</mapper>